{
  "extends": "@workspace/typescript-config/vite-react-library.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@repo/data-ops/*": ["./src/*"]
    },
    "types": ["@cloudflare/workers-types", "vitest/globals", "node"],
    "allowImportingTsExtensions": false,
    "declaration": true,
    "declarationMap": false,
    "noEmit": false,
    "outDir": "./dist"
  },
  "include": ["**/*.ts", "**/*.tsx", "scripts/listUsers.mjs"]
}
