{
  "cSpell.words": [
    "coef",
    "serie",
    "Serie",
    "yeko"
  ],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".vscode-insiders",
    "packages/**/src/tests/*.ts",
    "apps/**/src/tests/*.ts",
    "apps/**/src/**/*.test.tsx"
  ],
  "typescript.autoClosingTags": false
}
